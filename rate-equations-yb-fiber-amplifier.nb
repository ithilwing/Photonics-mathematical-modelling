Cell["Spectrum after the amplifier", "Text",
 CellChangeTimes->{{3.8389958715055637`*^9, 
  3.838995879875641*^9}},ExpressionUUID->"068fd76e-7572-4454-bb61-\
85a742e5240c"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"outSpectrum", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "\[Lambda]n"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "2"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", 
    RowBox[{"i", "\[LessEqual]", "\[Lambda]n"}], ",", 
    RowBox[{"i", "++"}], ",", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"Subscript", "[", 
        RowBox[{"funcPs", ",", "i"}], "]"}], "[", "z_", "]"}], "=", 
      RowBox[{
       RowBox[{"Ps", "[", 
        RowBox[{"z", ",", 
         RowBox[{"\[Lambda]smin", "+", 
          RowBox[{
           FractionBox[
            RowBox[{"(", 
             RowBox[{"\[Lambda]smax", "-", "\[Lambda]smin"}], ")"}], 
            "\[Lambda]n"], "\[Times]", 
           RowBox[{"(", 
            RowBox[{"i", "-", "1"}], ")"}]}]}]}], "]"}], "/.", "sols1"}]}], 
     ";", 
     RowBox[{
      RowBox[{"outSpectrum", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", "2"}], "]"}], "]"}], "=", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        RowBox[{"Subscript", "[", 
         RowBox[{"funcPs", ",", "i"}], "]"}], "[", "L", "]"}], "]"}]}], ";", 
     RowBox[{
      RowBox[{"outSpectrum", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", "1"}], "]"}], "]"}], "=", 
      RowBox[{"\[Lambda]smin", "+", 
       RowBox[{
        FractionBox[
         RowBox[{"(", 
          RowBox[{"\[Lambda]smax", "-", "\[Lambda]smin"}], ")"}], 
         "\[Lambda]n"], "\[Times]", 
        RowBox[{"(", 
         RowBox[{"i", "-", "1"}], ")"}]}]}]}]}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"outSpectrum", "=", 
   RowBox[{"Partition", "[", 
    RowBox[{
     RowBox[{"Flatten", "[", "outSpectrum", "]"}], ",", "2"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]"}], "Input",
